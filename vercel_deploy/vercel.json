{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "public/try.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/predict", "dest": "api/index.py" },
    { "src": "/", "dest": "api/index.py" }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  }
}